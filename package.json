{
  "name": "@dxsixpc/components",
  "version": "0.2.6",
  "description": "组件库",
  "homepage": "https://dxsixpc.github.io/components",
  "repository": "https://github.com/dxsixpc/components",
  "license": "MIT",
  "author": {
    "name": "dxsix",
    "email": "dxsixpc@gmail.com"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "lint": "eslint --cache --ext .js,.jsx,.mjs,.ts,.tsx src",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && pnpm run build",
    "start": "pnpm run dev"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@babel/runtime": "^7.21.5",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/modifiers": "^6.0.1",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/serialize": "^1.1.2",
    "@emotion/styled": "^11.11.0",
    "@emotion/utils": "^1.2.1",
    "@monaco-editor/react": "^4.5.1",
    "@rc-component/mini-decimal": "^1.0.1",
    "ahooks": "^3.7.7",
    "antd": "^5.4.7",
    "lodash": "^4.17.21",
    "monaco-editor": "^0.38.0",
    "prismjs": "^1.29.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-simple-code-editor": "^0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-react": "^7.18.6",
    "@dxsixpc/configs": "^1.0.5",
    "@dxsixpc/utils": "^1.0.0",
    "@emotion/babel-plugin": "^11.10.6",
    "@province-city-china/area": "^8.5.6",
    "@province-city-china/city": "^8.5.6",
    "@province-city-china/province": "^8.5.6",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "dumi": "^2.1.22",
    "eslint": "^8.40.0",
    "father": "^4.1.9",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-is": "^18.2.0",
    "styled-components": "^5.3.10",
    "stylelint": "^15.6.1",
    "typescript": "^5.0.4",
    "webpack": "^5.82.1"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
